<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>BiologySys</display-name>
  <servlet>
    <servlet-name>Living page</servlet-name>
    <jsp-file>/Living.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Living page</servlet-name>
    <url-pattern>/living</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Main page</servlet-name>
    <jsp-file>/Main.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Main page</servlet-name>
    <url-pattern>/main</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Classes page</servlet-name>
    <jsp-file>/Classes.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Classes page</servlet-name>
    <url-pattern>/classes</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Edit living page</servlet-name>
    <jsp-file>/EditLiving.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Edit living page</servlet-name>
    <url-pattern>/editliving</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Edit class page</servlet-name>
    <jsp-file>/EditClass.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Edit class page</servlet-name>
    <url-pattern>/editclass</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Add class page</servlet-name>
    <jsp-file>/AddClass.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Add class page</servlet-name>
    <url-pattern>/addclass</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Add living page</servlet-name>
    <jsp-file>/AddLiving.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Add living page</servlet-name>
    <url-pattern>/addliving</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Handler</servlet-name>
    <servlet-class>biosys.UpdateHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Handler</servlet-name>
    <url-pattern>/handleUpdate</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>biosys.Application</listener-class>
  </listener>
  <error-page>
    <exception-type>java.lang.Throwable</exception-type>
    <location>/Error.jsp</location>
  </error-page>
</web-app>